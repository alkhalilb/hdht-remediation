{
  "name": "hdht-remediation",
  "version": "1.0.0",
  "description": "Hypothesis-Driven History Taking Remediation App",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:app\"",
    "dev:server": "cd server && npm run dev",
    "dev:app": "cd app && npm run dev",
    "build": "cd app && npm run build",
    "install:all": "npm install && cd app && npm install && cd ../server && npm install"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
